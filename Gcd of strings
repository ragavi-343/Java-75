class Solution {
    public String gcdOfStrings(String str1, String str2) {
        if(!(str1+str2).equals(str2+str1))
        {
            return "";
        }
        int a=str1.length();
        int b=str2.length();
        while(b!=0)
            {
                int t=b;
                b=a%b;
                a=t;
            }
        int gcdlen=a;
        return str2.substring(0,gcdlen);
    }
}
