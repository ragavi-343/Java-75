class Solution {
    public String reverseWords(String s) {
        String[] sr=s.trim().split("\\s+");
        StringBuilder sc=new StringBuilder();
        for(int i=sr.length-1;i>=0;i--)
        {
            if(i==0)
            {
                sc.append(sr[i]);
            }
            else{
          sc.append(sr[i]);
          sc.append(' ');}
        }
        String r=sc.toString();
        return r;
    }
}
